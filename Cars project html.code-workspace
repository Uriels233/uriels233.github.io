  margin: 0;
  padding: 0;
  box-sizing: border-box;
}

body {
  height: 100vh;
  background-color: rgb(1, 0, 6);
  display: grid;
  place-items: center;
  font-family: "Inter", sans-serif;
  color: white;
}

